  <rules>
    <if equiprring="Prouesse Ring">
      <addtochat color="121">Skill Up Ring</addtochat>
      <equip when="precast|midcast|Aftercast|Idle|Resting|Engaged">
        <rring lock="true">Prouesse Ring</rring>
      </equip>
    </if>
    <!-- Autoset -->
    <equip when="resting" set="Resting" />
    <!-- Brew -->
    <if Buffactive="Transcendency">
      <cmd>input /macro set 7</cmd>
      <addtochat color="121">Brew Gear Locked</addtochat>
      <equip when="precast|midcast|Aftercast|Idle|Resting|Engaged" set="Brew"/>
      <Return/>
    </if>
    <elseif mode="OR" advanced='"$PDT"=="1"' buffactive="weak*">
      <addtochat color='121'>PDT Set Locked</addtochat>
      <equip when="Engaged|Aftercast|idle" set="PDT" />
      <if type="Weaponskill">
        <equip when="precast" set="%Spell"/>
      </if>
    </elseif>
    <elseif advanced='"$MDT"=="1"'>
      <addtochat Color="121">MDT Set Locked</addtochat>
      <equip when="Engaged|Aftercast|idle" set="MDT" />
    </elseif>
    <else>
      <!-- DD -->
      <if equipmain="$Sword|empty">
        <disable slot="Main|Sub|Ranged"/>
        <!-- Acc-->
        <if advanced='"$Mode"=="1"'>
          <if status="engaged">
            <equip when="engaged|AfterCast" set="TPACC" />
          </if>
          <else>
            <equip when="idle|AfterCast" set="Standard" />
          </else>
        </if>
        <else>
          <if status="engaged">
            <equip when="engaged|AfterCast" set="TP" />
          </if>
          <else>
            <equip when="idle|AfterCast" set="Standard" />
          </else>
        </else>
      </if>
      <!-- Range-->
      <elseif equipmain="$Dagger">
        <disable slot="Main|Sub|Ranged"/>
        <!-- Acc-->
        <if advanced='"$Mode"=="1"'>
          <if status="engaged">
            <equip when="engaged|AfterCast" set="RaACC" />
          </if>
          <else>
            <equip when="idle|AfterCast" set="Standard" />
          </else>
        </if>
        <else>
          <if status="engaged">
            <equip when="engaged|AfterCast" set="RATP" />
          </if>
          <else>
            <equip when="idle|AfterCast" set="Standard" />
          </else>
        </else>
        
      </elseif>
      <!-- QD-->
      <elseif equipmain="$Staff">
        <enable slot="Main|Sub"/>
        <if status="engaged">
          <equip when="engaged|AfterCast" set="TPAcc">
            <main>$QDStaff</main>
          </equip>
        </if>
        <else>
          <equip when="idle|AfterCast" set="Standard">
            <main>$QDStaff</main>
          </equip>
        </else>
      </elseif>
      <else>
        <addtochat color="121">Unknown Weapon</addtochat>
      </else>
    </else>
    <!-- Ranged Attack -->
    <if CommandPrefix="/range|/ra">
      <!-- Check Ammo-->
      <if notEquipAmmo="$TPBullet">
        <equip when="Precast">
          <ammo>$TPBullet</ammo>
        </equip>
      </if>
      <if EquipAmmo="$QDBullet">
        <cancelspell/>
        <addtochat color="121">Wrong Ammo</addtochat>
        <equip when="Precast">
          <ammo>$TPBullet</ammo>
        </equip>
      </if>
      <if buffactive="Barrage">
        <addtochat color="121">Barrage Set</addtochat>
        <equip when="precast" set="RATP"/>
      </if>
      <else>
        <equip when="precast" set="Snapshot"/>
        <if advanced='"$Mode"=="1"'>
          <midcastdelay delay="0.5"/>
          <equip when="midcast" set="RaACC"/>
          <addtochat color="121">RaAcc Set</addtochat>
        </if>
        <else>
          <equip when="midcast" set="RATP"/>
          <midcastdelay delay="0.5"/>
          <addtochat color="121">RATP Set</addtochat>
        </else>
      </else>
    </if>
    <if type="CorsairRoll">
      <equip when="Precast" set="PhantomRoll"/>
      <!-- Account for Luzaf's Ring-->
      <if advanced='"$Luzaf"=="1"'>
        <equip when="precast">
          <rring lock="true">Luzaf's Ring</rring>
        </equip>
        <addtochat color="121">Luzaf's Ring is ACTIVE</addtochat>
      </if>
      <!-- Account for No Luzaf's Ring-->
      <else>
        <addtochat color="121">Luzaf's Ring is NOT ACTIVE</addtochat>
      </else>
      <!-- Roll Enchancements -->
      <if spell="Courser*">
        <equip when="precast">
          <feet>Nvrch. Bottes +2</feet>
        </equip>
      </if>
      <elseif spell="Blitzer's Roll">
        <equip when="precast">
          <head>Nvrch. Tricorne +2</head>
        </equip>
      </elseif>
      <elseif spell="Tactician's Roll">
        <equip when="precast">
          <body>Nvrch. Frac +2</body>
        </equip>
      </elseif>
      <elseif spell="Allies' Roll">
        <equip when="precast">
          <hands>Nvrch. Gants +2</hands>
        </equip>
      </elseif>
    </if>
    <!-- Quick Draw -->
    <elseif type="CorsairShot">
      <!-- Handle Elemental Obi's -->
      <if advanced = '("%SpellElement" = "%WeatherElement" or "%SpellElement" = "%DayElement" or "%SpellElement" = "$StormElement")'>
        <if advanced='"$%SpellElementObi" = "1"'>
          <equip when="midcast">
            <waist lock="True">$%SpellElement-Obi</waist>
          </equip>
        </if>
      </if>
      <if spell="Light*|Dark*">
        <equip when="PreCast" set="QDACC" />
      </if>
      <else spell="Ice*|Wind*|Earth*|Thunder*|Water*|Fire*">
        <equip when="PreCast" set="QDMAB" />
      </else>
    </elseif>
    <elseif type="JobAbility">
      <if spell="Fold">
        <equip when="precast">
          <hands>Comm. Gants +2</hands>
        </equip>
      </if>
      <elseif spell="Wild Card">
        <equip when="precast">
          <feet>Comm. Bottes +2</feet>
        </equip>
      </elseif>
      <elseif spell="Random Deal">
        <equip when="precast">
          <body>Commodore Frac +2</body>
        </equip>
      </elseif>
      <elseif spell="Snake Eye">
        <equip when="precast">
          <legs>Comm. Culottes +2</legs>
        </equip>
      </elseif>
      <elseif spell="Cutting Cards">
      </elseif>
      <elseif spell="Triple Shot">
        <equip when="precast">
          <body>Nvrch. Frac +2</body>
        </equip>
      </elseif>
      <elseif spell="Double-Up">
        <equip when="Precast" set="PhantomRoll"/>
        <!-- Account for Luzaf's Ring-->
        <if advanced='"$Luzaf"=="1"'>
          <equip when="precast">
            <rring lock="true">Luzaf's Ring</rring>
          </equip>
          <addtochat color="121">Luzaf's Ring is ACTIVE</addtochat>
        </if>
      </elseif>
    </elseif>
    <!--Weapons Skills-->
    <elseif type="Weaponskill">
      <if status="engaged">
        <!-- Check TP-->
        <if tpgt="99">
          <!-- Ranged WS-->
          <if spell="Last Stand|Slug Shot|Wildfire|Leaden Salute|Numbing Shot|Detonator|Hot Shot|Sniper Shot|Split Shot">
            <!-- Check Ammo-->
            <if notEquipAmmo="$WSBullet">
              <equip when="Precast">
                <ammo>$WSBullet</ammo>
              </equip>
            </if>
            <!-- Check Distance -->
            <if advanced="%SpelltargetDistance &gt; 17.1" spell="Slug Shot|Last Stand|Leaden Salute|Wildfire|Detonator|Heavy Shot|Blast Shot">
              <cancelspell/>
              <Addtochat color="121">%SpellTargetName is Too Far for %Spell</Addtochat>
              <Return/>
            </if>
            <elseif advanced="%SpellTargetDistance &gt; 7.1" spell="Numbing Shot">
              <cancelspell/>
              <Addtochat color="121">%SpellTargetName is Too Far for %Spell</Addtochat>
              <Return/>
            </elseif>
            <!-- Add more ws-->
            <if spell="Wildfire">
              <equip when="precast|midcast" set="%Spell">
                <ammo>$WSBullet</ammo>
              </equip>
              <castdelay delay="0.03" />
              <!-- Account for Fire Obi and Zodiac Ring -->
              <if advanced='("%WeatherElement"="Fire") OR ("%DayElement"="Fire") OR ("$StormElement"="Fire")' >
                <equip when="precast|midcast">
                  <waist lock="yes">Karin Obi</waist>
                  <lring lock="yes">Zodiac Ring</lring>
                </equip>
              </if>
              <!-- Account for Athos Gloves-->
              <if advanced='("%DAY"="DarksDay") OR ("%DAY"="Earthsday")'>
                <equip when="precast|midcast">
                  <hands lock="yes">Athos's Gloves</hands>
                </equip>
              </if>
            </if>
            <elseif spell="Last Stand">
              <equip when="precast|midcast" set="%Spell">
                <ammo>$WSBullet</ammo>
              </equip>
            </elseif>
            <else>
              <equip when="precast|midcast" set="RAWS">
                <ammo>$WSBullet</ammo>
              </equip>
            </else>
          </if>
          <!-- Melee WS -->
          <else>
            <if advanced="%SpellTargetDistance &gt; 5">
              <cancelspell/>
              <addtochat color="121">%SpellTargetName is Too Far for %Spell.</addtochat>
              <Return/>
            </if>
            <else>
              <if spell="Exenterator">
                <equip when="precast" set="%Spell"/>
              </if>
              <elseif spell="Requiescat">
                <equip when="precast" set="%Spell"/>
              </elseif>
              <else>
                <equip when="precast" set="WS"/>
              </else>
            </else>
          </else>
        </if>
        <else>
          <cancelspell/>
          <addtochat color="121">Not Enough TP to use %Spell</addtochat>
        </else>
      </if>
      <else>
        <addtochat color="121">Must be engaged to WS. Engaging</addtochat>
        <cmd>input /attack</cmd>
      </else>
    </elseif>
    <elseif type="*Magic">
      <if spell="Cure*">
        <equip when="precast" set="Fastcast"/>
        <equip when="midcast" set="MND"/>
      </if>
      <elseif spell="Sneak" BuffActive="Sneak" advanced='"%target"="&lt;me&gt;"'>
        <midcastdelay delay="1" />
        <command when="MidCast">cancel 71</command>
      </elseif>
      <elseif spell="Stoneskin">
        <equip when="precast" set="MND"/>
        <midcastdelay delay="3" />
        <command when="MidCast">cancel 37</command>
      </elseif>
      <elseif spell="Sleep">
        <changespell>Light Shot</changespell>
      </elseif>
      <elseif spell="Dispel">
        <changespell>Dark Shot</changespell>
      </elseif>
    </elseif>
    <!-- Sub jobs -->
    <!-- WHM Subjob-->
    <if subjob="whm">
      <if advanced='"$AutoMacroset"=="1"'>
        <cmd when="aftercast">input /macro set $WHMMacroset</cmd>
      </if> 
    </if>
    <!-- NIN Subjob-->
    <elseif subjob="NIN">
      <if advanced='"$AutoMacroset"=="1"'>
        <cmd when="aftercast">input /macro set $NINMacroset</cmd>
      </if>
      <if spell="Utsusemi*">
        <equip when="precast" set="fastcast">
          <neck>Magoraga Beads</neck>
        </equip>
        <if spell="*Ichi" BuffActive="Copy Image*">
          <midcastdelay delay="2.5" />
          <command when="midcast">cancel 66</command>
          <command when="midcast">cancel 445</command>
          <command when="midcast">cancel 446</command>
          <command when="midcast">cancel 444</command>
        </if>
      </if>
      <if spell="Sneak">
        <changespell>Monomi: Ichi</changespell>
      </if>
      <if spell ="invisible">
        <changespell>Tonko: Ni</changespell>
      </if>
      <if spell="Monomi*|Tonko*">
        <equip when="Precast|midcast" set="fastcast" />
        <if Spell="Monomi*" BuffActive="Sneak">
          <midcastdelay delay="2.5" />
          <command when="midcast">cancel 71</command>
        </if>
      </if>
    </elseif>
    <elseif subjob="RNG">
      <if advanced='"$AutoMacroset"=="1"'>
        <cmd when="aftercast">input /macro set $RNGMacroset</cmd>
      </if>
    </elseif>
    <elseif subjob="BRD">
      <if advanced='"$AutoMacroset"=="1"'>
        <cmd when="aftercast">input /macro set $BRDMacroset</cmd>
      </if>
    </elseif>
    <elseif subjob="DNC">
      <if advanced='"$AutoMacroset"=="1"'>
        <cmd when="aftercast">input /macro set $DNCMacroset</cmd>
      </if>
      <if spell="Curing*">
        <equip when="precast" set="Waltz"/>
      </if>
      <elseif spell="*Step">
        <equip when="precast" set="Steps"/>
      </elseif>
      <elseif spell="*Flourish">
        <equip when="precast" set="Flourish"/>
      </elseif>
    </elseif>
    <elseif subjob="SCH">
      <if advanced='"$AutoMacroset"=="1"'>
        <cmd when="aftercast">input /macro set $SCHMacroset</cmd>
      </if>
    </elseif>
  </rules>
</spellcast>